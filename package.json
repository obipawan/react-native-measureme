{
  "name": "react-native-measureme",
  "version": "0.0.2",
  "description": "A HOC to make your React-Native components aware of their width and height",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel --presets=es2015,react --plugins=transform-object-rest-spread MeasureMeHOC.js --out-file dist/index.js",
    "prepare": "npm run build",
    "prepush": "npm run build",
    "precommit": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obipawan/react-native-measureme.git"
  },
  "keywords": [
    "react",
    "react-native",
    "component",
    "measure",
    "size",
    "dimensions",
    "size-aware"
  ],
  "author": "Pawan Kumar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/obipawan/react-native-measureme/issues"
  },
  "homepage": "https://github.com/obipawan/react-native-measureme#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "husky": "^3.0.4"
  }
}
